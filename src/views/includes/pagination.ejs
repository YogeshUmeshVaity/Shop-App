<!-- Page numbers -->
<% const currentPage = paginationData.currentPage %>
<% const previousPage = paginationData.previousPage %>
<% const nextPage = paginationData.nextPage %>
<% const lastPage = paginationData.lastPage %>
<% const hasPreviousPage = paginationData.hasPreviousPage %>
<% const hasNextPage = paginationData.hasNextPage %>
<section class="pagination">
    <% if (currentPage !== 1 && previousPage !== 1) { %>
        <a href="?page=1">1</a>
    <% } %>
    <% if (hasPreviousPage) { %>
        <a href="?page=<%= previousPage %>"><%= previousPage %></a>
    <% } %>
    <a href="?page=<%= currentPage %>" class="active"><%= currentPage %></a>
    <% if (hasNextPage) { %>
        <a href="?page=<%= nextPage %>"><%= nextPage %></a>
    <% } %>
    <% if (lastPage !== currentPage && nextPage !== lastPage) { %>
        <a href="?page=<%= lastPage %>"><%= lastPage %></a>
    <% } %>
</section>